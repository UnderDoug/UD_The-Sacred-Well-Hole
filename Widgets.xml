<?xml version="1.0" encoding="utf-8" ?>
<objects>

  <object Name="DaylightCell" Inherits="Widget">
    <part Name="DaylightCell" />
    <tag Name="ExcludeFromDynamicEncounters" />
    <tag Name="NoTwiddle" />
    <tag Name="NoPassByMessage" />
    <tag Name="ExcludeFromDynamicEncounters" Value="*noinherit" />
    <tag Name="IgnoreOccupationChecks" />
    <tag Name="HideInNearbyItemsList" />
  </object>

  <object Name="SmallDaylightCell" Inherits="DaylightCell">
    <part Name="DaylightCell" MaxRadius="2" />
    <tag Name="NoTwiddle" />
    <tag Name="NoPassByMessage" />
    <tag Name="IgnoreOccupationChecks" />
    <tag Name="HideInNearbyItemsList" />
  </object>

  <object Name="SelfObliterator" Inherits="Widget">
    <part Name="Render" DisplayName="self obliterator" RenderString="x" ColorString="&amp;K" />
    <part Name="ObliterateSelf" />
    <tag Name="ExcludeFromDynamicEncounters" />
    <tag Name="NoTwiddle" />
    <tag Name="NoPassByMessage" />
    <tag Name="IgnoreOccupationChecks" />
    <tag Name="HideInNearbyItemsList" />
  </object>
  
</objects>