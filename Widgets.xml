<?xml version="1.0" encoding="utf-8" ?>
<objects>
  <!-- 
      
      Zone mutators, Tile mutators, Multi-tile presets, Quest triggers, UI widgets
      
   -->
  <object Name="DaylightCell" Inherits="Widget">
    <part Name="DaylightCell" />
    <tag Name="ExcludeFromDynamicEncounters" />
    <tag Name="IgnoreOccupationChecks" />
  </object>

  <object Name="SmallDaylightCell" Inherits="DaylightCell">
    <part Name="DaylightCell" MaxRadius="2" />
  </object>

  <object Name="SelfSacrificeHelper" Inherits="Widget">
    <part Name="Render" DisplayName="[self sacrifice helper]" RenderString="x" ColorString="&amp;K" />
    <part Name="SelfSacrificeHelper" />
    <tag Name="ExcludeFromDynamicEncounters" />
    <tag Name="IgnoreOccupationChecks" />
  </object>
  
</objects>